root {

    // Hook into the root matcher to change rendering for JSON requests
    json = Kleisli.FusionFiles:Helper.RenderFileMatcher {
        fileExtension = 'json'
    }

    // Hook into the root matcher to change rendering for ICS requests
    ics = Kleisli.FusionFiles:Helper.RenderFileMatcher {
        fileExtension = 'ics'
    }

}
